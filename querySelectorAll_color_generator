    <script>
function getRandomRgbColor() {
  const r = Math.floor(Math.random() * 256);
  const g = Math.floor(Math.random() * 256);
  const b = Math.floor(Math.random() * 256);
  return `rgb(${r}, ${g}, ${b})`;
}
document.querySelector(".container").style.backgroundColor=getRandomRgbColor()
let boxes= document.querySelectorAll(".box");
boxes.forEach( (i) => {
    i.style.backgroundColor = getRandomRgbColor() ;
})
// console.log();

    </script>
</body>
